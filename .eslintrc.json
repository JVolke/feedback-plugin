{
    "parser": "vue-eslint-parser",
    "parserOptions": {
        "parser": "@babel/eslint-parser",
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/base",
        "plugin:vue/recommended"
    ],
    "globals": {
        "jQuery": false,
        "$": true,
        "App": false,
        "Vue": false,
        "vueEventHub": false,
        "$translate": false
    }
}
